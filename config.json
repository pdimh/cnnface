{
    "model_path": "data/train/",
    "gpu_mem_limit": 6700,
    "preprocessing": {
        "force_cpu": true,
        "output_path": "data/sample/",
        "fddb": {
            "annotation": "data/FDDB/annotations",
            "binary": "data/FDDB/bin"
        },
        "percentage": {
            "training": 85,
            "validation": 0
        },
        "widerface": {
            "annotation": "data/WIDER_FACE/wider_face_split",
            "training": "data/WIDER_FACE/WIDER_train/images",
            "validation": "data/WIDER_FACE/WIDER_val/images",
            "testing": "data/WIDER_FACE/WIDER_test/images"
        },
        "adapter": "WIDERFACE",
        "stage1": {
            "iou_threshold": 0.65,
            "min_score": 0.5,
            "pyramid_levels": 11,
            "min_face_size": 10
        },
        "stage2": {
            "iou_threshold": 0.65,
            "min_score": 0.5
        }
    },
    "training": {
        "path": "data/sample",
        "output_path": "data/train/",
        "force_cpu": false,
        "stage1": {
            "batch_size": 1000,
            "epochs": 300
        },
        "stage2": {
            "batch_size": 1000,
            "epochs": 600
        },
        "stage3": {
            "batch_size": 700,
            "epochs": 300
        }
    },
    "inference": {
        "min_face_size": 20
    }
}